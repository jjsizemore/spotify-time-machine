{
	"name": "spotify-time-machine",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"clean": "rm -rf .next",
		"debug": "pnpm run clean && pnpm run build && pnpm run dev",
		"dev": "next dev --turbopack --hostname 127.0.0.1",
		"build": "pnpm run css:build && next build",
		"start": "next start",
		"lint": "next lint",
		"css:build": "tailwindcss -i ./src/styles/main.css -o ./src/app/spotify.css",
		"css:watch": "tailwindcss -i ./src/styles/main.css -o ./src/app/spotify.css --watch",
		"postinstall": "prisma generate"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^1.6.0",
		"@prisma/client": "^6.7.0",
		"@tanstack/react-query": "^5.75.5",
		"@vercel/analytics": "^1.5.0",
		"@vercel/functions": "^2.0.3",
		"date-fns": "^2.30.0",
		"next": "15.3.2",
		"next-auth": "^4.24.11",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-icons": "^5.5.0",
		"spotify-web-api-node": "^5.0.2"
	},
	"devDependencies": {
		"@types/node": "^20.17.43",
		"@types/react": "19.1.3",
		"@types/react-dom": "19.1.3",
		"@types/spotify-web-api-node": "^5.0.11",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.3",
		"prisma": "^6.7.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.3"
	},
	"packageManager": "pnpm@10.10.0",
	"pnpm": {
		"overrides": {
			"@types/react": "19.1.3",
			"@types/react-dom": "19.1.3",
			"cookie@<0.7.0": ">=0.7.0"
		},
		"onlyBuiltDependencies": [
			"@prisma/client",
			"@prisma/engines",
			"esbuild",
			"prisma",
			"sharp"
		]
	}
}
