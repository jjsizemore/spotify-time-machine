{
	"name": "jermaines-spotify-time-machine",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"clean": "rm -rf .next node_modules/.cache && rm -rf node_modules && pnpm store prune",
		"clean:cache": "rm -rf .next node_modules/.cache .turbo",
		"clean:all": "rm -rf .next node_modules/.cache .turbo node_modules && pnpm store prune",
		"debug": "pnpm run clean:all && pnpm install && pnpm run build && pnpm run dev",
		"dev": "next dev --turbopack --hostname 127.0.0.1",
		"build": "next build",
		"start": "next start",
		"lint": "trunk check",
		"fmt": "trunk fmt",
		"postinstall": "flowbite-react patch",
		"type-check": "tsc --noEmit",
		"analyze": "ANALYZE=true next build"
	},
	"dependencies": {
		"@c15t/nextjs": "^1.4.2",
		"@next/third-parties": "^15.3.5",
		"@tailwindcss/cli": "^4.1.11",
		"@tailwindcss/postcss": "^4.1.11",
		"@tanstack/react-query": "^5.81.5",
		"@vercel/analytics": "^1.5.0",
		"@vercel/functions": "^2.2.3",
		"date-fns": "^4.1.0",
		"fflate": "^0.8.2",
		"flowbite": "^3.1.2",
		"flowbite-react": "^0.11.8",
		"next": "^15.3.5",
		"next-auth": "^4.24.11",
		"posthog-js": "^1.256.2",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-icons": "^5.5.0",
		"web-vitals": "^5.0.3",
		"zod": "^3.25.75"
	},
	"devDependencies": {
		"@types/node": "^24.0.3",
		"@types/react": "19.1.8",
		"@types/react-dom": "19.1.6",
		"postcss": "^8.5.6",
		"sharp": "^0.34.2",
		"tailwindcss": "^4.1.10",
		"typescript": "^5.8.3"
	},
	"packageManager": "pnpm@10.12.1",
	"pnpm": {
		"overrides": {
			"@types/react": "19.1.8",
			"@types/react-dom": "19.1.6",
			"cookie@<0.7.0": ">=0.7.0"
		},
		"onlyBuiltDependencies": ["core-js", "esbuild"]
	}
}
