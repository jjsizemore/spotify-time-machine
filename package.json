{
	"name": "jermaines-spotify-time-machine",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"clean": "rm -rf .next node_modules/.cache && rm -rf node_modules && pnpm store prune",
		"clean:cache": "rm -rf .next node_modules/.cache .turbo",
		"clean:all": "rm -rf .next node_modules/.cache .turbo node_modules && pnpm store prune",
		"debug": "pnpm run clean:all && pnpm install && pnpm run build && pnpm run dev",
		"dev": "next dev --hostname 127.0.0.1",
		"build": "next build",
		"start": "next start",
		"lint": "trunk check",
		"fmt": "trunk fmt",
		"type-check": "tsc --noEmit",
		"analyze": "ANALYZE=true next build",
		"check": "turbo lint type-check",
		"check:branding": "grep -RIn --exclude-dir=.next --exclude=**/output.css --exclude=**/*.svg --exclude=BRANDING.md '#1db954\\|#1DB954\\|#191414\\|#121212\\|#b3b3b3' src || true",
		"check:all": "turbo lint fmt type-check",
		"prebuild": "turbo type-check"
	},
	"dependencies": {
		"@c15t/nextjs": "^1.5.0",
		"@next/third-parties": "^15.5.2",
		"@tailwindcss/cli": "^4.1.12",
		"@tailwindcss/postcss": "^4.1.12",
		"@tanstack/react-query": "^5.85.9",
		"@vercel/analytics": "^1.5.0",
		"@vercel/functions": "^2.2.13",
		"date-fns": "^4.1.0",
		"fflate": "^0.8.2",
		"flowbite": "^3.1.2",
		"flowbite-react": "^0.12.8",
		"next": "^15.5.2",
		"next-auth": "^4.24.11",
		"posthog-js": "^1.261.4",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"react-icons": "^5.5.0",
		"web-vitals": "^5.1.0",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@types/node": "^24.7.0",
		"@types/react": "19.1.12",
		"@types/react-dom": "19.1.9",
		"postcss": "^8.5.6",
		"sharp": "^0.34.4",
		"tailwindcss": "^4.1.14",
		"turbo": "^2.5.8",
		"typescript": "^5.9.3"
	},
	"packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"@prisma/client",
			"@prisma/engines",
			"@tailwindcss/oxide",
			"esbuild",
			"prisma",
			"protobufjs",
			"sharp"
		]
	}
}
